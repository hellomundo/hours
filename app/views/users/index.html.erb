<% provide(:title, 'All users') %>
<h1>All users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Last Login</th>
      <th>Total Hours</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.name, user %></td>
      <td><%= user.created_at %></td>
      <td>0</td>
    </tr>
    <% end %>
  </tbody>
</table>